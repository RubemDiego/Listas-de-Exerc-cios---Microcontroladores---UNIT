#include <iostream>
#include <string>

using namespace std;

int main()
{
  int contador;               // Declara a variÃ¡vel contador.
  contador=1000;                 // contador recebe o valor 1.
  while (contador>=0)        // Enquanto contador for menor ou igual a 10.
  {
    cout << contador << endl; // Imprime contador.
    contador--;               // Incrementa contador em uma unidade.
  }
  return 0;
}
